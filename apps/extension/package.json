{
  "name": "mimic-extension",
  "version": "0.1.0",
  "description": "Mimic Extension",
  "type": "module",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{js,ts,tsx,vue} --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/**/*.{js,ts,tsx,vue} --fix --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{js,ts,vue,json,md}",
    "format:check": "prettier --check src/**/*.{js,ts,vue,json,md}",
    "quality": "npm run lint && npm run type-check && npm run format:check",
    "build:chrome:v3:unpacked": "webpack --config build/webpack.config.cjs --env browser=chrome --env manifestVersion=v3 --env target=unpacked",
    "build:chrome:v3:packed": "webpack --config build/webpack.config.cjs --env browser=chrome --env manifestVersion=v3 --env target=packed",
    "build:chrome:v3:store": "webpack --config build/webpack.config.cjs --env browser=chrome --env manifestVersion=v3 --env target=store",
    "build:chrome:v3": "run-p \"build:chrome:v3:unpacked {@}\" \"build:chrome:v3:packed {@}\" \"build:chrome:v3:store {@}\"",
    "build:edge:v3:unpacked": "webpack --config build/webpack.config.cjs --env browser=edge --env manifestVersion=v3 --env target=unpacked",
    "build:edge:v3:packed": "webpack --config build/webpack.config.cjs --env browser=edge --env manifestVersion=v3 --env target=packed",
    "build:edge:v3:store": "webpack --config build/webpack.config.cjs --env browser=edge --env manifestVersion=v3 --env target=store",
    "build:edge:v3": "run-p \"build:edge:v3:unpacked {@}\" \"build:edge:v3:packed {@}\" \"build:edge:v3:store {@}\"",
    "build:clean": "rimraf dist",
    "build:dev": "run-s build:clean && run-p \"build:chrome:v3:unpacked {@}\" \"build:edge:v3:unpacked {@}\" --",
    "build:prod": "run-s build:clean && run-p \"build:chrome:v3 {@}\" \"build:edge:v3 {@}\" --"
  },
  "keywords": [],
  "author": "sagi",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jshint": "^2.12.4",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "archiver": "^7.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "crx": "^5.0.1",
    "css-loader": "^7.1.2",
    "fs-extra": "^11.3.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "5.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "web-ext": "^0.0.1",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.12.1",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.6",
    "mimic-sdk": "workspace:*",
    "@mimic-sdk/core": "workspace:*",
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/openai": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "codemirror": "^6.0.2",
    "jimp": "^1.6.0",
    "jshint": "^2.13.6",
    "langchain": "^1.2.3",
    "lucide-react": "^0.562.0",
    "openai": "^6.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "thememirror": "^2.0.1",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.5",
    "zone.js": "^0.16.0"
  },
  "packageManager": "pnpm@10.26.2"
}
