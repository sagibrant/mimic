{
  "name": "agentjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build::chrome::v2::unpacked": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v2 --env target=unpacked",
    "build::chrome::v2::packed": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v2 --env target=packed",
    "build::chrome::v2::store": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v2 --env target=store",
    "build::chrome::v2": "run-p \"build::chrome::v2::unpacked -- {@}\" \"build::chrome::v2::packed -- {@}\" \"build::chrome::v2::store -- {@}\" --",
    "build::chrome::v3::unpacked": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v3 --env target=unpacked",
    "build::chrome::v3::packed": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v3 --env target=packed",
    "build::chrome::v3::store": "webpack --config build/webpack.config.js --env browser=chrome --env manifestVersion=v3 --env target=store",
    "build::chrome::v3": "run-p \"build::chrome::v3::unpacked -- {@}\" \"build::chrome::v3::packed -- {@}\" \"build::chrome::v3::store -- {@}\" --",
    "build::chrome": "run-p \"build::chrome::v2 -- {@}\" \"build::chrome::v3 -- {@}\" --",
    "build::edge::v2::unpacked": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v2 --env target=unpacked",
    "build::edge::v2::packed": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v2 --env target=packed",
    "build::edge::v2::store": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v2 --env target=store",
    "build::edge::v2": "run-p \"build::edge::v2::unpacked -- {@}\" \"build::edge::v2::packed -- {@}\" \"build::edge::v2::store -- {@}\" --",
    "build::edge::v3::unpacked": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v3 --env target=unpacked",
    "build::edge::v3::packed": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v3 --env target=packed",
    "build::edge::v3::store": "webpack --config build/webpack.config.js --env browser=edge --env manifestVersion=v3 --env target=store",
    "build::edge::v3": "run-p \"build::edge::v3::unpacked -- {@}\" \"build::edge::v3::packed -- {@}\" \"build::edge::v3::store -- {@}\" --",
    "build::edge": "run-p \"build::edge::v2 -- {@}\" \"build::edge::v3 -- {@}\" --",
    "build::firefox::v2::unpacked": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v2 --env target=unpacked",
    "build::firefox::v2::packed": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v2 --env target=packed",
    "build::firefox::v2::store": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v2 --env target=store",
    "build::firefox::v2": "run-p \"build::firefox::v2::unpacked -- {@}\" \"build::firefox::v2::store -- {@}\" --",
    "build::firefox::v3::unpacked": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v3 --env target=unpacked",
    "build::firefox::v3::packed": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v3 --env target=packed",
    "build::firefox::v3::store": "webpack --config build/webpack.config.js --env browser=firefox --env manifestVersion=v3 --env target=store",
    "build::firefox::v3": "run-p \"build::firefox::v3::unpacked -- {@}\" \"build::firefox::v3::store -- {@}\" --",
    "build::firefox": "run-p \"build::firefox::v2 -- {@}\" \"build::firefox::v3 -- {@}\" --",
    "build::clean": "rimraf dist",
    "build::dev": "run-s build::clean && run-p \"build::chrome -- {@}\" \"build::edge -- {@}\" \"build::firefox -- {@}\" --",
    "build::prod": "run-s build::clean && run-p \"build::chrome -- {@}\" \"build::edge -- {@}\" \"build::firefox -- {@}\" \"build::firefox::v2::packed -- {@}\" \"build::firefox::v3::packed -- {@}\" --"
  },
  "keywords": [],
  "author": "sagi",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler-cli": "^19.2.14",
    "@ngtools/webpack": "^19.2.14",
    "@types/chrome": "^0.1.1",
    "@types/node": "20.12.12",
    "@types/firefox-webext-browser": "^120.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "archiver": "^7.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "html-webpack-plugin": "5.6.0",
    "crx": "^5.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "fs-extra": "^11.3.0",
    "html-loader": "^5.1.0",
    "raw-loader": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "~5.8.3",
    "uuid": "^11.1.0",
    "web-ext": "^8.9.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.14",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "json-rpc-2.0": "^1.7.1",
    "rxjs": "^7.8.1",
    "zone.js": "^0.15.0"
  }
}
