{
	"name": "Gogogo",
	"version": "2025.4.8.1",
	"description": "Automate your browser with AI",
	"manifest_version": 3,
	"minimum_chrome_version": "95",
	"icons": {
		"16": "resources/icon_color_16x16.png",
		"32": "resources/icon_color_32x32.png",
		"48": "resources/icon_color_48x48.png",
		"128": "resources/icon_color_128x128.png"
	},
	"action": {
		"default_icon": {
			"16": "resources/icon_color_16x16.png",
			"32": "resources/icon_color_32x32.png",
			"48": "resources/icon_color_48x48.png",
			"128": "resources/icon_color_128x128.png"
		},
		"default_title": "Gogogo",
		"default_popup": "ui/action/index.html"
	},
	"options_ui": {
		"page": "ui/options/index.html",
		"open_in_tab": true
	},
	"side_panel": {
		"default_path": "ui/sidebar/index.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"match_about_blank": true,
			"js": [
				"content.js"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; worker-src 'self';"
	},
	"permissions": [
		"tabs",
		"debugger",
		"cookies",
		"browsingData",
		"storage",
		"webNavigation",
		"nativeMessaging"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"resources/*.*",
				"assets/*.*",
				"assets/css/*.*",
				"ui/*.*",
				"ui/action/*.*",
				"ui/options/*.*",
				"ui/sidebar/*.*"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}