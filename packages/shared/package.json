{
  "name": "@gogogo/shared",
  "private": true,
  "version": "0.1.0",
  "description": "Shared utilities for gogogo project",
  "repository": "https://github.com/qagogogo/gogogo",
  "homepage": "https://qagogogo.github.io/gogogo/",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "browser": "./dist/browser/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.mjs",
      "browser": "./dist/browser/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rimraf dist && rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "shared",
    "utils",
    "typescript",
    "chrome-extension",
    "node",
    "web"
  ],
  "author": "Sagi",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {}
}
