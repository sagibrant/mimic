{
  "name": "gogogo-internal",
  "private": true,
  "version": "0.1.0",
  "description": "Gogogo enables reliable end-to-end automation on your browser.",
  "type": "module",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "prepare": "husky",
    "lint:prettier": "prettier --write \"**/*.{js, ts, mjs, cjs, json, tsx, css, less, scss, vue, html, md}\"",
    "lint:eslint": "eslint",
    "lint:spellcheck": "cspell lint \"(packages|apps)/**/*.{js,ts,mjs,cjs,json,css,less,scss,vue,html,md}\"",
    "build:shared": "pnpm --prefix packages/shared build",
    "build:browser-sdk": "pnpm --prefix packages/browser-sdk build",
    "build:extension:dev": "pnpm --prefix apps/extension build:dev --",
    "build:extension:prod": "pnpm --prefix apps/extension build:prod --",
    "build:test": "pnpm --prefix tests/extension build",
    "build:dev": "pnpm build:test && pnpm build:shared && pnpm build:browser-sdk && pnpm build:extension:dev",
    "build:prod": "pnpm build:test && pnpm build:shared && pnpm build:browser-sdk && pnpm build:extension:prod",
    "build": "pnpm build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagibrant/gogogo.git"
  },
  "keywords": [],
  "author": "Sagi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sagibrant/gogogo/issues"
  },
  "homepage": "https://github.com/sagibrant/gogogo#readme",
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=11.6.2",
    "pnpm": ">=10.26.2"
  },
  "devDependencies": {
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "cspell": "^9.4.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
